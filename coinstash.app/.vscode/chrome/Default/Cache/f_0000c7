      var fallbackImageName = "https://secure.img-cdn.mediaplex.com/0/710/229501/103014_FEED_10404_Display_October_Fallbacks_300x75.jpg"

      function get_browser_info() {
            var ua = navigator.userAgent,
                  tem, M = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
            if (/trident/i.test(M[1])) {
                  tem = /\brv[ :]+(\d+)/g.exec(ua) || [];
                  return {
                        name: 'IE',
                        version: (tem[1] || '')
                  };
            }
            if (M[1] === 'Chrome') {
                  tem = ua.match(/\bOPR\/(\d+)/)
                  if (tem != null) {
                        return {
                              name: 'Opera',
                              version: tem[1]
                        };
                  }
            }
            M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
            if ((tem = ua.match(/version\/(\d+)/i)) != null) {
                  M.splice(1, 1, tem[1]);
            }
            return {
                  name: M[0],
                  version: M[1]
            };
      }

            function showFallback() {
            var mainbody = document.getElementById('fallback');
            mainbody.setAttribute('style', 'padding:0; margin:0;');
            mainbody.innerHTML = '<a href="http://uk.at.atwola.com/adlink/1065/3468237/0/2018/AdId=18008666;BnId=1;itime=240485581;nodecode=yes;impref=14992404853347870665;imprefseq=16047436749347184;imprefts=1499240485;link=http://rover.ebay.com/rover/1/710-229501-47862-2/4?mpt=2486&ff5=&ff6=&ff7=&ff8=&ff9=&ff19=&ext_id=&ir_DAP_A1=4294967295&siteid=3&ff10=101349799&ipn=admain2&ff14=unknown&ff20=72&mpcr=101349799&rvr_id=1249864357594&ck=27419_main" target="_blank" style="padding:0; margin:0; border:0;" ><img src="'+fallbackImageName+'" width="1272" height="328" style="padding:0; margin:0; border:0;"></a>';
            document.getElementById('mainwrapper').setAttribute('style', 'display:none')
      }


      if (get_browser_info().name.indexOf('IE') && get_browser_info().version <= 9 && get_browser_info().name != "Safari") {
            //IE 8 Fallback
            showFallback();
      }


      function animate() {
            TweenLite.to(_id("intro"), 1.4, { top: "75px", ease: Power2.easeOut});
      }

