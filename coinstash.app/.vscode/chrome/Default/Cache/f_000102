    <script type="text/javascript">
    var discountNull = "&nbsp;";
    var titleMaxLength = 30;


    // Wordings:
    var almGoneWording = "ALMOST GONE"; // Almost Gone Items
    var freeShipWording = "Free delivery"; // Free Shipping
    var prePercentages = "";
    var afterPercentages = "% OFF ";
    var currentOffer = "";
    var disableScript = false;
    var fallbackImageName = "FFED_https://secure.img-cdn.mediaplex.com/0/710/229504/106634_11469_Display_Spring_Deals_Fallback.jpg"

    function get_browser_info() {
        var ua = navigator.userAgent,
            tem, M = ua.match(/(opera|chrome|safari|firefox|msie|trident(?=\/))\/?\s*(\d+)/i) || [];
        if (/trident/i.test(M[1])) {
            tem = /\brv[ :]+(\d+)/g.exec(ua) || [];
            return {
                name: 'IE',
                version: (tem[1] || '')
            };
        }
        if (M[1] === 'Chrome') {
            tem = ua.match(/\bOPR\/(\d+)/)
            if (tem != null) {
                return {
                    name: 'Opera',
                    version: tem[1]
                };
            }
        }
        M = M[2] ? [M[1], M[2]] : [navigator.appName, navigator.appVersion, '-?'];
        if ((tem = ua.match(/version\/(\d+)/i)) != null) {
            M.splice(1, 1, tem[1]);
        }
        return {
            name: M[0],
            version: M[1]
        };
    }

    var fallbackW = document.getElementById('mainwrapper'),
        style = window.getComputedStyle(fallbackW),
        width = style.getPropertyValue('width'),
        height = style.getPropertyValue('height');
    var fallbackW = style.getPropertyValue('width');
    var fallbackH = style.getPropertyValue('height');

    function showFallback() {
        var mainbody = document.getElementById('fallback');
        mainbody.setAttribute('style', 'padding:0; margin:0;');
        mainbody.innerHTML = '<a href="https://uk.at.atwola.com/adlink/1065/4935803/0/154/AdId=18141350;BnId=1;itime=240929802;kp=294540;nodecode=yes;impref=14992409293331248598;imprefseq=37721001366134206;imprefts=1499240929;link=http://rover.ebay.com/rover/1/710-229504-51250-7/4?mpt=2928&ff5=&ff6=&ff7=&ff8=&ff9=&ff19=TACTIC&ext_id=&ir_DAP_A1=0&siteid=3&ff10=101565768&ipn=admain2&ff13=1&ff14=unknown&ff20=97&ff14=unknown&ff20=97&mpcr=101565768&rvr_id=1249857561206&ck=28473_main" target="_blank" style="padding:0; margin:0; border:0;" ><img src="' + fallbackImageName + '" width="' + fallbackW + '" height="' + fallbackH + '" style="padding:0; margin:0; border:0;"></a>';
        document.getElementById('mainwrapper').setAttribute('style', 'display:none')
    }


    if (get_browser_info().name.indexOf('IE') && get_browser_info().version <= 9 && get_browser_info().name != "Safari") {
        //IE 8 Fallback
        showFallback();
    }

    function animate() {
        TweenLite.to(_id("intro"), 0.5, {
            right: "160px",
            ease: Power2.easeOut
        });
    }
